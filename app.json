{
  "expo": {
    "name": "Roadtrip",
    "slug": "Roadtrip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/roadtrip.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/roadtrip-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#74C8B0"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/roadtrip.png",
        "backgroundColor": "#74C8B0"
      },
      "package": "com.bugrarslan.Roadtrip"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
