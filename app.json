{
  "expo": {
    "name": "Roadtrip",
    "slug": "Roadtrip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/roadtrip.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/roadtrip-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#74C8B0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bugrarslan.Roadtrip"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/roadtrip.png",
        "backgroundColor": "#74C8B0"
      },
      "package": "com.bugrarslan.Roadtrip"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/Outfit-Bold.ttf",
            "assets/fonts/Outfit-Medium.ttf",
            "assets/fonts/Outfit-Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f2b7414e-0dbe-4e34-827b-333a783887d8"
      }
    }
  }
}
